<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:tools="http://schemas.android.com/tools"
package="fortunefundadmin">

<application
    android:name="com.ffs.fortunefundadmin.FortuneFundApp"
    android:allowBackup="true"
    android:dataExtractionRules="@xml/data_extraction_rules"
    android:fullBackupContent="@xml/backup_rules"
    android:icon="@mipmap/ic_launcher"
    android:label="@string/app_name"
    android:roundIcon="@mipmap/ic_launcher_round"
    android:supportsRtl="true"
    android:theme="@style/Theme.FortuneFund"
    android:usesCleartextTraffic="true"
    tools:targetApi="31">
    <activity
        android:name="com.ffs.fortunefundadmin.ui.splash.SplashActivity"
        android:configChanges="orientation|keyboardHidden|screenSize"
        android:exported="true"
        android:label="@string/title_activity_splash"
        android:theme="@style/Theme.FortuneFund.Fullscreen">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity>
    <activity
        android:name="com.ffs.fortunefundadmin.MainActivity"
        android:exported="true"
        android:label="@string/app_name">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity>
    <activity
        android:name="com.ffs.fortunefundadmin.ui.login.LoginActivity"
        android:exported="false"
        android:label="Login" />

    <activity
        android:name="com.ffs.fortunefundadmin.ui.login.RegisterActivity"
        android:exported="false"
        android:label="Create Account" />

    <meta-data
        android:name="preloaded_fonts"
        android:resource="@array/preloaded_fonts" />

    <activity
        android:name="com.ffs.fortunefundadmin.ui.home.AddInvestmentPlanActivity"
        android:exported="false"

        />
</application>
</manifest>
